{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["jest", "node"],
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "include": ["js-tests/**/*.ts"],
  "exclude": ["node_modules"]
}
